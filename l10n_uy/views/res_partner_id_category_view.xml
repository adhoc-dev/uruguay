<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_partner_id_category_form" model="ir.ui.view">
        <field name="name">res.partner.id_category.form</field>
        <field name="model">res.partner.id_category</field>
       <field name="inherit_id" ref="l10n_ar_partner.view_partner_id_category_form"/>
        <field name="arch" type="xml">
            <field name="afip_code" position="after">
                <field name="l10n_uy_dgi_code"/>
            </field>
            <field name="afip_code" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
        </field>
    </record>

    <record id="view_partner_id_category_tree" model="ir.ui.view">
        <field name="name">res.partner.id_category.tree</field>
        <field name="model">res.partner.id_category</field>
        <field name="inherit_id" ref="l10n_ar_partner.view_partner_id_category_tree"/>
        <field name="arch" type="xml">
            <field name="afip_code" position="after">
                <field name="l10n_uy_dgi_code"/>
            </field>
            <field name="afip_code" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
        </field>
    </record>

</odoo>
